    62                                  ; signed comparison examples
    63                                  ; jg  - jump if greater (signed)
    64                                  ; jl  - jump if smaller (signed)
    65                                  ; jge - jump if greater or equal (signed)
    66                                  ; jle - jump if smaller or equal (signed)
    67                                  ; je  - jump if equal (zf=1)
    68                                  ; jne - jump if not equal (zf=0)
    69                                  ; -------------------------------
    70                                  
    71                                  ; ---- jg example ----
    72 00000042 B80500                  mov ax,5
    73 00000045 BB0300                  mov bx,3
    74 00000048 39D8                    cmp ax,bx
    75 0000004A 7F3E                    jg signedGreater5_3
    76 0000004C 90                      nop
    77                                  
    78                                  ; ---- jl example ----
    79 0000004D B8FBFF                  mov ax,-5
    80 00000050 BB0300                  mov bx,3
    81 00000053 39D8                    cmp ax,bx
    82 00000055 7C34                    jl signedSmallerMinus5_3
    83 00000057 90                      nop
    84                                  
    85                                  ; ---- jge example ----
    86 00000058 B80500                  mov ax,5
    87 0000005B BB0500                  mov bx,5
    88 0000005E 39D8                    cmp ax,bx
    89 00000060 7D2A                    jge signedGreaterOrEqual5_5
    90 00000062 90                      nop
    91                                  
    92                                  ; ---- jle example ----
    93 00000063 B8FBFF                  mov ax,-5
    94 00000066 BB0300                  mov bx,3
    95 00000069 39D8                    cmp ax,bx
    96 0000006B 7E20                    jle signedSmallerOrEqualMinus5_3
    97 0000006D 90                      nop
    98                                  
