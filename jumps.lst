     9                                  ; -------------------------------
    10                                  ; unsigned comparison examples
    11                                  ; ja  - jump if above (cf=0 & zf=0)
    12                                  ; jb  - jump if smaller (cf=1)
    13                                  ; jae - jump if above or equal (cf=0)
    14                                  ; jbe - jump if smaller or equal (cf=1 or zf=1)
    15                                  ; je  - jump if equal (zf=1)
    16                                  ; jne - jump if not equal (zf=0)
    17                                  ; -------------------------------
    18                                  
    19                                  ; ---- ja example ----
    20 00000000 B80500                  mov ax,5
    21 00000003 BB0300                  mov bx,3
    22 00000006 39D8                    cmp ax,bx
    23 00000008 777A                    ja above5_3
    24 0000000A 90                      nop
    25                                  
    26                                  ; ---- jb example ----
    27 0000000B B80300                  mov ax,3
    28 0000000E BB0500                  mov bx,5
    29 00000011 39D8                    cmp ax,bx
    30 00000013 7270                    jb smaller3_5
    31 00000015 90                      nop
    32                                  
    33                                  ; ---- jbe example ----
    34 00000016 B80300                  mov ax,3
    35 00000019 BB0500                  mov bx,5
    36 0000001C 39D8                    cmp ax,bx
    37 0000001E 7666                    jbe smallerOrEqual3_5
    38 00000020 90                      nop
    39                                  
    40                                  ; ---- jae example ----
    41 00000021 B80500                  mov ax,5
    42 00000024 BB0500                  mov bx,5
    43 00000027 39D8                    cmp ax,bx
    44 00000029 735C                    jae aboveOrEqual5_5
    45 0000002B 90                      nop
    46                                  
    47                                  ; ---- je example ----
    48 0000002C B80500                  mov ax,5
    49 0000002F BB0500                  mov bx,5
    50 00000032 39D8                    cmp ax,bx
    51 00000034 7452                    je equal5_5
    52 00000036 90                      nop
    53                                  
    54                                  ; ---- jne example ----
    55 00000037 B80500                  mov ax,5
    56 0000003A BB0300                  mov bx,3
    57 0000003D 39D8                    cmp ax,bx
    58 0000003F 7548                    jne notEqual5_3
